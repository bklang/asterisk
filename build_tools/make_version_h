#!/bin/sh
if [ ! -f ../.flavor ]; then
    EXTRA=""
else
    aadkver=`cat ../.version`
    aadkflavor=`cat ../.flavor`
    EXTRA=" (${aadkflavor} ${aadkver})"
fi
cat << END
/*
 * version.h 
 * Automatically generated
 */
#define ASTERISK_VERSION "${ASTERISKVERSION}${EXTRA}"
#define ASTERISK_VERSION_NUM ${ASTERISKVERSIONNUM}

END
